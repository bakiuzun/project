<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" spacing="10" alignment="CENTER"  fx:controller="com.example.controller.ConnectedController" >
    <TableView fx:id="tamagotchiTableView">
        <columns>
            <!-- Define your attribute columns here -->
           <TableColumn fx:id="attributeNameColumn" text="Attribute Name" prefWidth="150">
    <cellValueFactory>
        <PropertyValueFactory property="AttributeName" />
    </cellValueFactory>
</TableColumn>

<TableColumn fx:id="attributeValueColumn" text="Attribute Value" prefWidth="150">
    <cellValueFactory>
        <PropertyValueFactory property="AttributeValue" />
    </cellValueFactory>
</TableColumn>
        </columns>
    </TableView>
</VBox>
